NODE countt
SQL >
    WITH
        (select regex from emoji_regex) as emoji_regex_,
        extractAll(tweet, assumeNotNull(emoji_regex_)) as emojis
    SELECT
        toStartOfHour(date) date,
        search_term,
        arrayJoin(emojis) emoji,
        count() count
    FROM tweets
    GROUP BY date, search_term, emoji

TYPE Materialized
DATASOURCE emoji_count